class Foo {
  Foo() {
  }

  void printStuff() {
    doPrint();
  }

  virtual void doPrint()
  {
    print("Foo");
  }
  
  int x;
}

class Bar : Foo {
  Bar() {
  }
  
  virtual void doPrint()
  {
    print("Bar");
  }
}

void main()
{
  Foo foo = new Foo;
  Bar bar = new Bar;
  foo.printStuff();
  bar.printStuff();
}

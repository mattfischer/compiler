int strLength(string s)
{
  int i=0;
  while(s[i] != '\0') {
	i++;
  }

  return i;
}

string cat(string x, string y)
{
  int xlen = strLength(x);
  int ylen = strLength(y);
  string result = new string(xlen + ylen + 1);
  for(int i=0; i<xlen; i++) {
    result[i] = x[i];
  }
  for(int i=0; i<ylen; i++) {
    result[xlen + i] = y[i];
  }
  result[xlen + ylen] = '\0';
  return result;
}

void main(int x)
{
	print(cat("ABC", "DEF"));
}
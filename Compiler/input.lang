class Foo {
  Foo() {
  }

  virtual void doPrint()
  {
    print("Foo");
  }
}

class Bar : Foo {
  Bar() {
  }
  
  virtual void doPrint()
  {
    print("Bar");
  }
}

void printStuff(Foo foo) {
    foo.doPrint();
}

void main()
{
  Foo foo = new Foo;
  Bar bar = new Bar;
  printStuff(foo);
  printStuff(bar);
}
